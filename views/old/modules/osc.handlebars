<div class="container container-fullsize container-padding module module-osc" style='background-color:#56009C;color:#FFFF00'>

	<header class='header'>
		
		<a href="/play" class='left'>
			<div class="ic ic_overview"></div>
		</a>

		
		<h2 class="header_title center">{{module.title}}</h2>
		
		<div class="right ic_live_container 
			{{#if isLive}}
			ic_active
			{{else}}
			ic_inactive
			{{/if}}
			">
				<span class='ic_live_image'></span>
				<span class='ic_title ic_live_title'>LIVE</span>
		</div>
	</header>

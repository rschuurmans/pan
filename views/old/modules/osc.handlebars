	{{#each module.parameters}}
		
			{{#ifCond type "===" "wavetype"}}
				<div class="radio-slider">
					{{#each wavetypes}}
						<input type="radio" id="input-{{@index}}" name='toggle_option' {{#ifCond @index "===" 0}} checked {{/ifCond}}>

						<label for="input-{{@index}}"><span>{{type}}</span></label>
					{{/each}}

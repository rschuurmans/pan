{{> notification}}


<div class='pagetransition splashtransition-active container container-padding-lg container-yellow fn-alert text-center text-black above container-fixed fn-changepage-page ' order='0' origin='up' active='true' current-page='alert'>
	{{> pages/alert alert}}
</div>


<div class="pagetransition splashtransition-active container-fixed fn-changepage-page container-yellow above text-black text-center fn-hide-support" current-page='calibrate' active='true' order="1" origin='null'>
		{{> pages/calibrate}}
		
</div>


<div class="pagetransition container container-fixed container-padding-lg fn-changepage-page" current-page='filters' active='false'  order="2" origin='null'>
		{{> tipBox tips.0.text}}
		{{> pages/filters this}}
	
</div>

<div class="pagetransition container container-fixed container-padding-lg fn-changepage-page" current-page='osc' active='false'  order="3" origin='null'>
	
		{{> pages/sources this}}
	
</div>


<div class='hide'>
			{{> tipBox tips.0.text}}
	<div class='video-container'>
		      <video class='fn-video' id='video' width='100%' height='auto' preload autoplay loop muted controls></video>
		      <canvas class='fn-canvas' id='canvas' width='100%' height='auto'></canvas>
		    </div>
</div>


